{
  "prefix": "COLOR::",
  "colors": [
    "RED", "BLUE", "PURPLE", "YELLOW", "GREEN"
  ],
  "patterns": [
    "p = (:RED)-[:CALLS*]->(:BLUE) WHERE NOT any(n in nodes(p) WHERE n:PURPLE)",
    "(:GREEN)-[:CALLS*]->(:RED)",
    "(:YELLOW)-->(x) WHERE NOT x:YELLOW"
  ],
  "executor": "executors/noop_adapter.sh"
}
